/************************************************************ JAVA ************************************************/
//Approach-1 Brute Force
//T.C : O(n^m), where n = number of words, m = length of pref
//S.C : O(n^m)
class Solution {
    public int prefixCount(String[] words, String pref) {
        int count = 0;
        for(int i = 0; i<words.length; i++){
            if(words[i].indexOf(pref)==0){
                count++;
            }

        }
        return count;
    }
}
